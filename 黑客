idk9.Parent = Frame
idk9.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
idk9.Position = UDim2.new(0.69, 0, 0.2, 0)
idk9.Size = UDim2.new(0, 94, 0, 35)
