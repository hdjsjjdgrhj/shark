idk3.Parent = Frame
idk3.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
idk3.Position = UDim2.new(0.34, 0, 0.2, 0)
idk3.Size = UDim2.new(0, 94, 0, 35)
